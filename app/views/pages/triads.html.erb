  <canvas style="width: 100%; height: 200px;" resize="true" id="canvas-1" style="background:black"></canvas>

</div>
<div class="row">
  <div class="col-lg-4">
    <div class="bs-component">
      <div class="panel panel-default">
        <h4 class="topPanel panel-heading"><span>Select Single Notes</span></h4>
        <div class="panel-body">
          <button id="playKey" class ="btn btn-purple">Play</button>
          <button id="stopKey" class ="btn btn-danger">Stop</button>

          <select name="mySelect" id="selectedKey">
            <option value="233.082">A#</option>
            <option value="246.942">B</option>
            <option value="261.626">C</option>
            <option value="277.183">C#</option>
            <option value="293.665">D</option>
            <option value="311.127">D#</option>
            <option value="329.628">E</option>
            <option value="349.228">F</option>
            <option value="369.994">F#</option>
            <option value="391.995">G</option>
            <option value="415.305">G#</option>
            <option value="440.000">A</option>
          </select>
        </div>
      </div>

      <div class="panel panel-default">
        <h4 class="topPanel panel-heading"><span>Input any Note</span></h4>
        <div class="panel-body">
          <span>
            <h4 class="sameLine">Key</h4>
            <input id="inputKey" type="text" name="Please enter key" placeholder=" C, C#, A..." value="">
          </span>
          <span>
            <h4 class="sameLine sameLineOctave">Octave</h4>
            <!-- <input id="octave" type="text" name="Please enter key" placeholder="1...7" value=""> -->
            <select id="selectedOctave">
              <!-- <option value=4>1</option>
              <option value=4>2</option> -->
              <option selected="selected" value=3>3</option>
              <option value=4>4</option>
              <option value=5>5</option>
              <option value=6>6</option>
              <option value=7>7</option>
              <option value=8>8</option>
            </select>
          </span>

            <span class="inline">
              <h4>Scale Type</h4>
              <!-- <input id="octave" type="text" name="Please enter key" placeholder="1...7" value=""> -->
              <select id="scaleType">
                <option value="major">major</option>
                <option>minor</option>
                <option>harmonicminor</option>
                <option>melodicminor</option>
                <option>ionian</option>
                <option>dorian</option>
                <option>phrygian</option>
                <option>lydian</option>
                <option>mixolydian</option>
                <option>locrian</option>
                <option>chromatic</option>
                <option>aeolian</option>
              </select>
            </span>
          <!-- <input id="inputKeyButton" type="Play" name="submit" value="submit"> -->
          <h4>Scale: <span id="scaleOutput"></span></h4>
          <button id="playTriad" class ="btn btn-purple" value="submit">Play Triad</button>
          <button id="playScale" class ="btn btn-purple" value="submit">Play Scale</button>
        </div>
      </div>

      <div class="panel panel-default">
        <h4 class="topPanel panel-heading"><span>Find a Complementary Harmony</span></h4>
        <div class="panel-body">
          <span>
            <h4 class="sameLine">Root Note: </h4>
            <span class="rootNote"></span>
          </span>
          <span>
            <h4 class="sameLine sameLineOctave">Octave</h4>
            <select id="selectedOctave2">
              <option value=2>2</option>
              <option value=3>3</option>
              <option selected="selected" value=4>4</option>
              <option value=5>5</option>
              <option value=6>6</option>
              <option value=7>7</option>
              <option value=8>8</option>
            </select>
          </span>

            <span class="inline">
              <h4>Chord Inversions</h4>
              <select id="inversionType">
                <option>Root-Position</option>
                <option selected="selected">Second-Inversion</option>
                <option>Third-Inversion</option>
              </select>
            </span>
            <p></p>
            <button id="playCompTriad" class ="btn btn-blue" value="submit">Play Repeating Triad</button>
          </div>
        </div>
      </div>
    </div>

